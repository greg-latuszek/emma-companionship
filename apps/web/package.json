{
  "name": "@emma/web",
  "version": "1.0.0",
  "private": true,
  "description": "Emma Companionship - Next.js fullstack application",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config=jest.integration.config.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rm -rf .next dist *.tsbuildinfo"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@prisma/client": "^5.8.1",
    "next-auth": "5.0.0-beta.4",
    "@auth/prisma-adapter": "^1.4.0",
    "zod": "^3.22.4",
    "@emma/shared-types": "workspace:*",
    "@emma/ui": "workspace:*",
    "@emma/config": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@next/eslint-config-next": "^14.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "typescript": "^5.3.3",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "prisma": "^5.8.1",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "tsx": "^4.7.0"
  }
}
